<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import java.net.URL?>
<BorderPane fx:id="baseLayout" prefHeight="400.0" prefWidth="800.0" styleClass="layout-background"
            xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="easv.ticketapp.gui.BaseLayoutController">
    <left>
        <fx:include source="main-menu.fxml"/>
    </left>

    <center>
        <VBox fx:id="contentContainer" BorderPane.alignment="CENTER" VBox.vgrow="ALWAYS">
        </VBox>
    </center>
    <stylesheets>
        <URL value="@../../css/main.css"/>
    </stylesheets>
</BorderPane>
